{
    "name": "cosname/flarum-cos-misc",
    "description": "Miscellaneous customizations for COS Flarum.",
    "type": "flarum-extension",
    "keywords": [
        "misc",
        "customization"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yixuan Qiu",
            "email": "yixuan.qiu@cos.name"
        }
    ],
    "require": {
        "flarum/core": "^0.1.0-beta.6"
    },
    "autoload": {
        "psr-4": {
            "Cosname\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "COS Misc",
            "icon": {
                "image": "icon.png",
                "backgroundSize": "contain",
                "backgroundRepeat": "no-repeat",
                "backgroundPosition": "center",
                "backgroundColor": "#fff",
                "color": "#fff"
            }
        }
    }
}
